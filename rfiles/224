<script type="text/javascript">
var playing = false;
$(document).ready(function() {
var height = $('#slider .column').first().outerHeight(true);
$('#music_body').animate({'height': height}, 500);
$(".details").each(function() {
$(this).css('bottom', -$(this).outerHeight()+"px");
$(".play_button", $(this).parent()).css("top", ((($(this).parent().height() - $(this).outerHeight()) / 2) - 25) + "px");
});
$(".music_head a").click(function(e) {
var id = $(this).index();
var width = $('#music_body').width();
$('#slider').animate({'left': -1 * id * width}, 500);
var height = $($('#slider .column')[id]).outerHeight(true);
$('#music_body').animate({'height': height}, 500);
$('.music_head a.selected').removeClass('selected');
$(this).addClass('selected');
e.preventDefault();
return false;
});
$(".clip_player").bind("loadedmetadata", function() {
$(".play_button", $(this).parent()).removeClass("hidden");
}).bind("ended", function() {
$(".play_button", $(this).parent()).removeClass("pause");
$(".clip_progress", $(this).parent()).width(0);
clearTimeout(clip_timer);
});
$(".play_button").click(function() {
if ($(this).hasClass("pause")) {
$(".clip_player", $(this).parent())[0].currentTime = 0;
$(".clip_player", $(this).parent())[0].pause();
$(".clip_progress", $(this).parent()).width(0);
$(this).removeClass("pause");
clearTimeout(clip_timer);
} else {
$(".play_button.pause, #play_button.pause, #play_button.buffer").click();
$(".clip_player", $(this).parent())[0].play();
$(this).addClass("pause");
var code = $(this).closest(".song").attr("id");
clip_timer = setInterval(function() { updateDuration(code); }, 100);
}
});
});
$(window).resize(function() {
var id = $(".music_head .selected").index();
var width = $('#music_body').width();
$('#slider').css('left', -1 * id * width);
var height = $($('#slider .column')[id]).outerHeight(true);
$('#music_body').css('height', height);
});
function updateDuration(code) {
var player = $("#"+code+" .clip_player")[0];
var duration = player.duration;
if (duration > -1) {
var current = player.currentTime;
var perc = (current / duration) * 100;
$("#"+code+" .clip_progress").width(perc+"%");
}
}
