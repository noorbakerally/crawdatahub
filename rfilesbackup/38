    function add_css(url, media) {
      var head = document.getElementsByTagName('head')[0];
      var link = document.createElement('link');
      link.rel = 'stylesheet';
      link.type = 'text/css';
      link.href = url;
      link.media = media;
      head.appendChild(link);
    }

    function add_script(url) {
      var head = document.getElementsByTagName('head')[0];
      var script = document.createElement('script');
      script.type = 'text/javascript';
      script.src = url;
      head.appendChild(script);
    }

    function jswitch() {
      $('#flat').remove();
      $('#jsbody').show();
      $('#timeBrowser1Content').hide();

      $("#metadataContainer").append(
        createMetadataSnippet(
          "tna", infos, true, texts));
      $("#calendarSlider").append(
        createTimeBrowserSnippet(infos['instances'], texts));
      position_date_tiles();
      $('#loaderContainer1').hide('fade', 'slow', function() {
        $('#timeBrowser1Content').slideDown(500);
      });
      
      scrollToDay(20131202175712);
      

      // TODO see how to prevent from building it all, instead of hiding it
      // after the fact
      if ("False" == "False") {
        $("#iframeContainer").width("100%");
        $("#timeBrowseColumn").remove();
      }
    }

    
    function jsdata() {
      add_css('/tna/template_sel/css/main.css', 'screen');
      add_css('/tna/template_sel/css/print.css', 'print');
    }

    $(function() {
      "use strict";
      if ($('html').is('.ie6, .ie7, .ie8')) {
        // don't replace the dom and keep the nojs version
      } else {
        jsdata();
        if ($('html').is('.ie9'))
          add_css('/tna/template_sel/css/debug-ie9.css', 'all');
        else if ($('html').is('.ie10'))
          add_css('/tna/template_sel/css/debug-ie10.css', 'all');
        jswitch();
      }
      $("#theWebpage").load(function() {
        $("#loaderContainer2").hide('fade', 'slow', function() {
          $("#loaderContainer2").remove();
        });
      });
    });
    
  </script>
