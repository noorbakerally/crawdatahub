    <sioc:has_creator rdf:resource="http://my.gnoss.com/en/class/IES-ICARO-1GCM-2011-12/profile/Alba-Cosme-Moreno/" />
    <sioc:reply_of rdf:resource="http://red.gnoss.com/en/community/MPV-APADRINA/resource/aportacion-a-nuestra-ceo/2f90c137-1d35-4945-946a-89d771d06c5a/comentario/234f1c8c-005a-407f-a2c3-e7607835cac6" />
