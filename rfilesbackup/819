  <foaf:Person rdf:about="http://my.gnoss.com/en/profile/profesora-laclasedelaura/">
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal" xmlns:xml="http://www.w3.org/XML/1998/namespace" xml:space="preserve"><![CDATA[Profesora · Laura colorado ortiz]]></rdfs:label>
    <foaf:firstName rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal"><![CDATA[Laura]]></foaf:firstName>
    <foaf:familyName rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal"><![CDATA[colorado ortiz]]></foaf:familyName>
