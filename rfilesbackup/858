  <sioc_t:Tag rdf:about="http://miguiadeviajes.net/en/community/guiasdeviaje/search/tag/hogar">
    <rdfs:label rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal" xmlns:xml="http://www.w3.org/XML/1998/namespace" xml:space="preserve"><![CDATA[hogar]]></rdfs:label>
