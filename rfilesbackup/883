<script type="text/javascript" src="/static/js/d3.min.js"></script>
<script type="text/javascript">
var margin = {top: 10, right: 5, bottom: 0, left: 5},
    width = 255 - margin.left - margin.right,
    height = 80 - margin.top - margin.bottom;

var bisectYear = d3.bisector(function(d) { return d.year; }).left;

var x = d3.scale.linear()
    .range([0, width]);

var y = d3.scale.linear()
    .range([height, 0]);

var xAxis = d3.svg.axis()
    .scale(x)
    .orient("bottom");

var yAxis = d3.svg.axis()
    .scale(y)
    .orient("left");

var line = d3.svg.line()
    .x(function(d) { return x(d.year); })
    .y(function(d) { return y(d.value); });

var area = d3.svg.area()
    .x(function(d) { return x(d.year); })
    .y0(height)
    .y1(function(d) { return y(d.value); });

var tooltip = d3.select("#chart").append("div")
    .attr("class", "tooltip")
    .style("opacity", 1e-6);

var svg = d3.select("#chart").append("svg")
    .attr("width", width + margin.left + margin.right)
    .attr("height", height + margin.top + margin.bottom)
    .append("g")
    .attr("transform", "translate(" + margin.left + "," + margin.top + ")");

var cur_url = location.pathname;
var lang = $("#lang").val();
var st_num = 0;
if (cur_url.indexOf("/search") > -1) {
	st_num = cur_url.indexOf("/search");
	cur_url = cur_url.slice(st_num);
    cur_url = cur_url.replace(/\/search/, "/year");
    if (cur_url.slice(-1) != "/") {
	    cur_url += "/";
	}
    cur_url += location.search;
} else if (cur_url.indexOf("/opensearch") > -1) {
	st_num = cur_url.indexOf("/opensearch");
	cur_url = cur_url.slice(st_num);
    cur_url = cur_url.replace(/\/opensearch/, "/year");
    if (cur_url.slice(-1) != "/") {
        cur_url += "/";
    }
    cur_url += location.search;
}
if (cur_url.indexOf("/") < 0) {
    cur_url = "/" + cur_url;
}
cur_url = "/" + lang + cur_url;

d3.json(cur_url, function(error, data) {

    data.forEach(function(d) {
      d.year = d.year;
      d.value = +d.value;
    });

    data.sort(function(a, b) {
      return a.year - b.year;
    });

    x.domain([data[0].year, data[data.length - 1].year]);
    y.domain([0, d3.max(data, function(d) { return d.value; })]);


    var clip = svg.append("defs").append("svg:clipPath")
        .attr("id", "clip")
        .append("svg:rect")
        .attr("id", "clip-rect")
        .attr("x", "0")
        .attr("y", "0")
        .attr("width", width)
        .attr("height", height);


    svg.append("path")
        .datum(data)
        .attr("class", "line")
        .attr("clip-path", "url(#clip)")
        .attr("d", line);

    svg.append("path")
        .datum(data)
        .attr("class", "area")
        .attr("clip-path", "url(#clip)")
        .attr("d", area);

    var focus = svg.append("g")
        .attr("class", "focus")
        .style("display", "none");

    focus.append("circle")
        .attr("r", 4.5);

    svg.append("rect")
        .attr("class", "overlay")
        .attr("width", width)
        .attr("height", height)
        .on("mouseover", function() {
          focus.style("display", null);
          tooltip.transition()
          .duration(500)
          .style("opacity", 1);
        })
        .on("mouseout", function() {
          focus.style("display", "none");
          tooltip.transition()
          .duration(500)
          .style("opacity", 1e-6);
        })
        .on("mousemove", mousemove);

    function mousemove() {
      var x0 = x.invert(d3.mouse(this)[0]),
          i = bisectYear(data, x0, 1),
          d0 = data[i - 1],
          d1 = data[i],
          d = x0 - d0.year > d1.year - x0 ? d1 : d0;
      focus.attr("transform", "translate(" + x(d.year) + "," + y(d.value) + ")");

      tooltip.text(d.year + "(" + d.value + ")")
      .style("left", x(d.year) + "px")
      .style("top", y(d.value) - 25 + "px");
    }
});

// slider
$(function() {

  $.getJSON(cur_url,function(data){

    var startYear = Number(data[0].year),
        endYear = Number(data[data.length-1].year);

    $("#startYear").val(startYear);
    $("#endYear").val(endYear);

    $( "#slider" ).slider({
      range: true,
      min: startYear,
      max: endYear,
      values: [startYear,endYear],
      slide: function( event, ui ) {

        var maxv = d3.min([ui.values[1], data.length]);
        var minv = d3.max([ui.values[0], 0]);

        x.domain([ui.values[0], ui.values[1]]);

        svg.transition().duration(750)
        .select(".area").attr("d", area);

        svg.transition().duration(750)
        .select(".focus").attr("d", line(data));

        svg.transition().duration(750)
        .select(".line").attr("d", line(data));

        // facet search
        search(ui.values[0],ui.values[1]);

      }
    });
  });
});

function search(start,end){
  $("#startYear").val(start);
  $("#endYear").val(end);
}
</script>



