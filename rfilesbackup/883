<div class="panel-default" id="facet_fy">
  <h3>Project Period (FY)</h3>
  <div class="panel-body award-year">
    <div id="chart"></div>
    <div id="slider-wrap">
      <div id="slider"></div>
      <div class="form-inline">
        <div class="form-group"><input type="text" id="startYear" class="form-control" maxlength="4"></div> - <div class="form-group"><input type="text" id="endYear" class="form-control" maxlength="4"></div>
        <div class="form-group">
          <input type="button" class="btn btn-sm btn-default update" value="&#xf021;" onclick="return facetYear('search');" />
          <input type="button" class="btn btn-sm btn-default clear" value="Clear" onclick="return facetYear('clear');" />
        </div>
