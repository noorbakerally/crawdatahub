<script type="text/javascript">
//<![CDATA[
$(document).ready(function(){
    $(".checkAll").on("change", function() {
        $("." + this.id).prop("checked", this.checked);
        if ($(".chk_group:checked").length == 0) {
        	$("#file_btn").addClass("invisible");
        } else {
        	$("#file_btn").removeClass("invisible");
        }
    });
    
    $(".chk_group").on("change", function() {
		if ($(".chk_group:checked").length == 0) {
        	$("#file_btn").addClass("invisible");
        } else {
        	$("#file_btn").removeClass("invisible");
        }
    });
    
    if ($("select[name=ct]").val() == 'all') {
		$(".chk_group").prop('checked', false);
		$(".checkAll").prop('checked', false);
		$(".chk_group").prop("disabled", true);
		$(".checkAll").prop("disabled", true);
		$("#file_btn").removeClass("invisible");
    }
    
    $("#ct").on("change", function() {
    	if ($("select[name=ct]").val() == 'all') {
    		$(".chk_group").prop('checked', false);
    		$(".checkAll").prop('checked', false);
    		$(".chk_group").prop("disabled", true);
    		$(".checkAll").prop("disabled", true);
    		$("#file_btn").removeClass("invisible");
	    } else {
	        $(".chk_group").prop("disabled", false);
	        $(".checkAll").prop("disabled", false);
	        if ($(".chk_group:checked").length == 0) {
	        	$("#file_btn").addClass("invisible");
	        }
	    }
    });
});
//]]>
</script>



